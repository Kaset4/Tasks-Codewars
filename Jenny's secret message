class Program
{
 static void Main(string[] args)
    {
        Program program = new Program();
        greet("asdasd");
    }
    public static string greet(string name)
    {

        if (name == "Johnny")
        {
            Console.WriteLine("Hello, my love!");
            return "Hello, my love!";
        }
        else
        {
            return "Hello, " + name + "!";
        }
    }
}
